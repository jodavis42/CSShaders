[Math.FixedArrayPrimitive(typeof(float), 20)]
public struct FixedArrayFloat20
{
    public uint Length() { return 20u; }
    

    [Shader.ArrayGetIntrinsic]
    public extern float GetValue(int index);
    [Shader.ArraySetIntrinsic]
    public extern void SetValue(int index, float value);

    public float this[int key]
    {
      get { return GetValue(key); }
      set { SetValue(key, value); }
        //get => GetValue(key);
        //set => SetValue(key, value);
    }
}

