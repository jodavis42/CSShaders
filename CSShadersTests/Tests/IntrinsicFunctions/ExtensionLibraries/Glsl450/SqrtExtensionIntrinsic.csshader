[Math.VectorPrimitive(typeof(float), 3)]
public struct TestVector3Primitive
{
    float X, Y, Z;
}

struct TestIntrinsics
{
  [Shader.SimpleExtensionIntrinsic("GLSL.std.450", "Sqrt")] public extern static float Sqrt(float value);
  [Shader.SimpleExtensionIntrinsic("GLSL.std.450", "Sqrt")] public extern static TestVector3Primitive Sqrt(TestVector3Primitive value);
}

[Shader.UnitTest][Shader.Pixel]
struct SinExtensionIntrinsic
{
    [Shader.EntryPoint]
    public void Main()
    {
        var floatResult = TestIntrinsics.Sqrt(1.0f);
        var float3Reslt = TestIntrinsics.Sqrt(new TestVector3Primitive());
    }
}
